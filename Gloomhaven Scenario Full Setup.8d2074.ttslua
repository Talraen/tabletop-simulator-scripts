function onSave()
	local DataToSave = {
		NumCharacters = NumCharacters
		,NumScenario = NumScenario
		,TypeMonster = TypeMonster
		,TypeMap = TypeMap
		,Tiles_MonsterUnspawned = Tiles_MonsterUnspawned
	}

	SavedData = JSON.encode(DataToSave)

	return SavedData
end

function onLoad(SavedData)
	if SavedData ~= "" then
		local LoadedData = JSON.decode(SavedData)

		NumCharacters = LoadedData.NumCharacters
		NumScenario = LoadedData.NumScenario
		TypeMonster = LoadedData.TypeMonster
		TypeMap = LoadedData.TypeMap
		Tiles_MonsterUnspawned = LoadedData.Tiles_MonsterUnspawned
	end

	if NumCharacters == nil then NumCharacters = 2 end
	if NumScenario == nil then NumScenario = 1 end
	if TypeMonster == nil then TypeMonster = 1 end
	if TypeMap == nil then TypeMap = 1 end
	if Tiles_MonsterUnspawned == nil then Tiles_MonsterUnspawned = {} end


	createButtons_onLoad()

	numCharacters_Update()
	numScenario_Update()
	typeMonster_Update()
	typeMap_Update()
end




