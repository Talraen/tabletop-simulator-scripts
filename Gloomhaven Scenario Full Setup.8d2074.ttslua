	---------------------------------------
	--Spawn Map, Overlay, Start, Door Tiles
	---------------------------------------
	function spawnMapTiles()
		wait(Delay_Spawning)

		for _,tMapTile in ipairs(MapTiles) do
			--Add tile to Monster Unspawned
			table.insert(Tiles_MonsterUnspawned, tMapTile.tile)


			--Spawn Map Tile
			for _,tBag in ipairs(getObjectFromGUID(Bag_MapTiles_GUID).getObjects()) do
				if tBag.name == tMapTile.tile then
					tMapTile.guid			= tBag.guid
					tMapTile.smooth			= false
					tMapTile.callback		= "spawnCallback"
					tMapTile.callback_owner	= self
					tMapTile.params			= {lock = true}

					getObjectFromGUID(Bag_MapTiles_GUID).takeObject(tMapTile)
					break
				end
			end

			-- Scenario Sections
			-- If Scenario Sections Exist
			if tMapTile.scenarioSections ~= nil then
				-- For Each Page, Spawn Page
				for _,tSection in pairs(tMapTile.scenarioSections) do
					if tSection.params == nil then
						tSection.params = {}
					end

					tSection.params.lock = true

					if tSection.hidden ~= nil then
						tSection.params.buttonTheme = "Conclusion"
						tSection.params.buttonLabel = tSection.buttonLabel
						tSection.params.colourTint	= {r=0/255,g=0/255,b=0/255}
						tSection.rotation = {0.00, 180.00, 180.00}
					else
						tSection.rotation = {0.00, 180.00, 0.00}
					end

					tSection.type				= "Custom_Token"
					tSection.sound				= false
					tSection.snap_to_grid		= true
					tSection.callback			= "spawnCallback"
					tSection.callback_owner	= self

					spawnObject(tSection).setCustomObject(tSection)

				end
			end

			--Overlay Tiles (not Start, Doors)
			--If Overlays exist
			if tMapTile.overlayTiles ~= nil then
				--For each bag type
				for _,tBag in ipairs(tMapTile.overlayTiles) do
					--Find the right bag GUID
					local Bag_Source = getObjectFromGUID(bag_TypeOverlay_GUID(tBag.bag))

					--For each tile type
					for _,tType in ipairs(tBag.type) do
						--Reset Infinite Bag
						Bag_InfiniteBag.reset()

						--Put one into Infinite (Put one back if not from an infinite source)
						if tType.name == "Coin 1" then
							--Coin 1
							Bag_InfiniteBag.putObject(
								getObjectFromGUID(Infinite_Coin1_GUID).takeObject({
									smooth	= false
								})
							)
						else
							--Find the right tile in bag
							for _,tBag_Source in ipairs(Bag_Source.getObjects()) do
								if tBag_Source.name == tType.name then
									--Take one
									Bag_InfiniteBag.putObject(
										Bag_Source.takeObject({
											guid	= tBag_Source.guid
											,smooth	= false
										})
									)

									--Put one back
									Bag_Source.putObject(
										Bag_InfiniteBag.takeObject({
											smooth	= false
										})
									)
									break
								end
							end
						end

						--For each placement
						for _,tTiles in ipairs(tType.tiles) do
							tTiles.smooth			= false
							tTiles.callback			= "spawnCallback"
							tTiles.callback_owner	= self

							if tTiles.params == nil then tTiles.params = {} end
							if tTiles.params.lock == nil then
								tTiles.params.lock = true
							end
							if
								tTiles.params.buttonTheme == "Treasure"
								and tTiles.params.script == nil
								and tTiles.params.buttonLabel ~= "G"
							then
								tTiles.params.script = ""
..								"local NumTreasure = " .. tTiles.params.buttonLabel
							end

							Bag_InfiniteBag.takeObject(tTiles)
						end
					end
				end
			end


			--Start Tile
			if tMapTile.startTiles ~= nil then
				--Reset Infinite Bag
				Bag_InfiniteBag.reset()

				--Find the right bag GUID
				local Bag_Source = getObjectFromGUID(Bag_Corridors_GUID)

				--Put one into Infinite (Put one back if not from an infinite source)
				for _,tBag_Source in ipairs(Bag_Source.getObjects()) do
					if tBag_Source.name == tMapTile.startTiles.name then
						--Take one
						Bag_InfiniteBag.putObject(
							Bag_Source.takeObject({
								guid	= tBag_Source.guid
								,smooth	= false
							})
						)

						--Put one back
						Bag_Source.putObject(
							Bag_InfiniteBag.takeObject({
								smooth	= false
							})
						)
						break
					end
				end

				--For each placement
				for _,tTiles in ipairs(tMapTile.startTiles.tiles) do
					tTiles.smooth			= false
					tTiles.callback			= "spawnCallback"
					tTiles.callback_owner	= self

					if tTiles.params == nil then tTiles.params = {} end
					if tTiles.params.buttonTheme == nil then
						tTiles.params.buttonTheme = "Start"
					end
					if tTiles.params.lock == nil then
						tTiles.params.lock = true
					end

					Bag_InfiniteBag.takeObject(tTiles)
				end
			end
		end



		if DoorTiles ~= nil then
			--Find the right bag GUID
			local Bag_Source = getObjectFromGUID(Bag_Doors_GUID)

			for _,tType in ipairs(DoorTiles) do
				--Reset Infinite Bag
				Bag_InfiniteBag.reset()

				--Put one into Infinite (Put one back if not from an infinite source)
				for _,tBag_Source in ipairs(Bag_Source.getObjects()) do
					if tBag_Source.name == tType.name then
						--Take one
						Bag_InfiniteBag.putObject(
							Bag_Source.takeObject({
								guid	= tBag_Source.guid
								,smooth	= false
							})
						)

						--Put one back
						Bag_Source.putObject(
							Bag_InfiniteBag.takeObject({
								smooth	= false
							})
						)
						break
					end
				end

				--For each placement
				for _,tTiles in ipairs(tType.tiles) do
					tTiles.smooth			= false
					tTiles.callback			= "spawnCallback"
					tTiles.callback_owner	= self

					if tTiles.params == nil then tTiles.params = {} end
					if tTiles.params.buttonTheme == nil then
						tTiles.params.buttonTheme = "Door"
					end
					if tTiles.params.lock == nil then
						tTiles.params.lock = true
					end
					if tTiles.params.script == nil then
						local ScriptConstruct = ""
..						"local NumScenario = " .. NumScenario
..						"\nlocal AdjacentTiles = {"

						for iAdjacent,tAdjacent in ipairs(tTiles.adjacentTiles) do
							if iAdjacent ~= 1 then
								ScriptConstruct = ScriptConstruct .. ","
							end
							ScriptConstruct = ScriptConstruct .. "'" .. tAdjacent .. "'"
						end

						ScriptConstruct = ScriptConstruct
..						"}"

						tTiles.params.script = ScriptConstruct
					end

					Bag_InfiniteBag.takeObject(tTiles)
				end
			end
		end

		return 1
	end
	startLuaCoroutine(self, "spawnMapTiles")



	------------------------------
	--Clean up and Delete all Bags
	------------------------------
	function deleteObjects()
		Timer.destroy("BagCleanUp" .. self.getGUID())
		local AllObjects = getAllObjects()
		for _,obj in ipairs(AllObjects) do
			if string.sub(obj.getName(),1,10) == "CreateMap_" then
				destroyObject(obj)
			end
		end
		IsBagSpawned = 0
	end
	Timer.create({
		identifier		= "BagCleanUp" .. self.getGUID()
		,function_name	= "deleteObjects"
		,delay			= Delay_Delete
	})
end




--Spawn Scenario Page
