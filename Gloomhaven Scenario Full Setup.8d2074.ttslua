--Scenario 100 Rotate
---------------------
function rotate100(value)
	if IsBusy == 1 then
		broadcastToAll(String_Busy, {1,1,1})
		return
	end

	function rotateTiles()
		IsBusy = 1
		local F1 = getObjectFromGUID("d7fe48")
		--local zone = getObjectFromGUID("2ea4d8")
		local clockwise = getObjectFromGUID(clockwise_GUID)
		local clockwise = getObjectFromGUID(anticlockwise_GUID)
		local clockwise = getObjectFromGUID(pillar_GUID)
		local position = {}
		local z = {}
		z.type				= "ScriptingTrigger"
		z.scale			= {16.51, 5.10, 16.44}
		z.rotation		= {0.00, 180.00, 0.00}
		z.position		= {0.01, 3.00, 5.30}
		zone = spawnObject(z)
		zone.setCustomObject(z)

		wait(0.5)
		local toAttach = zone.getObjects()

		for _,zoneObject in ipairs(toAttach) do
			position = zoneObject.getPosition()
			if position.y > 1.68 then
				if zoneObject ~= F1 then
					F1.addAttachment(zoneObject)
				end
			end
		end

		if scenarioState == nil then
			if value == -1 then
				scenarioState = 1
			else
				scenarioState = 2
			end
		else
			scenarioState = scenarioState + value
		end

		if scenarioState > 4 then
			scenarioState = 1
		elseif scenarioState < 1 then
			scenarioState = 4
		end

		if scenarioState == 1 then
			F1.setPosition({-2.62, 1.71, 4.54})
			F1.setRotation({0.00, 269.95, 180.00})
			Grid.type = 2
		end

		if scenarioState == 2 then
			F1.setPosition({0.00, 1.71, 7.87})
			F1.setRotation({0.00, 359.91, 180.00})
			Grid.type = 3
		end

		if scenarioState == 3 then
			F1.setPosition({2.63, 1.71, 4.57})
			F1.setRotation({0.00, 90.00, 180.00})
			Grid.type = 2
		end

		if scenarioState == 4 then
			F1.setPosition({0.00, 1.71, 2.56})
			F1.setRotation({0.00, 180.01, 180.00})
			Grid.type = 3
		end

		F1.removeAttachments()
		zone.destruct()
		IsBusy = 0
		return 1
	end
	startLuaCoroutine(self, "rotateTiles", value)
end

function FCChallengeStairs(value)
	function upStairs()
		isBusy = 1
		local scripts = getObjectFromGUID("7ef9dd")
		local z = {}
		z.type				= "ScriptingTrigger"
		z.position			= {-0.05, 4.34, 2.71}
		z.rotation		= {0.00, 180.00, 0.00}
		z.scale		= {12.77, 5.10, 10.53}
		zone = spawnObject(z)
		zone.setCustomObject(z)
		wait(0.5)

		local inZone = zone.getObjects()

		for _,zoneObject in ipairs(inZone) do
			if zoneObject.getLock() or zoneObject.getName() == "Coin 1" then
				zoneObject.destruct()
			end
		end

		zone.destruct()
		isBusy = 0
		local params = { adjacentTiles = {"N1", value}, numScenario = 300}

		self.call('revealRoom',params)
		scripts.call('cleanMonster')
		return 1
	end
	startLuaCoroutine(self, "upStairs", value)
end

--------------------------------------------
--Reveal Rooms after initial setup from door
--------------------------------------------
function revealRoom(params)
	if IsBusy == 1 then
		broadcastToAll(String_Busy, {1,1,1})
		return
	end
	IsBusy = 1

	local AllObjects = getAllObjects()
	local ScenarioInformation = deepCopy(ScenarioInformation_All[params.numScenario])
	local params_DoorTiles = {}
	local params_MapTiles = {}
	local params_Monsters = {}




	-----------
	--Flip Door
	-----------
	if params.doorTile_GUID then
		function flipDoorTile()
			wait(Delay_Spawning+1)
			local position = getObjectFromGUID(params.doorTile_GUID).getPosition()
			position.y = position.y + 0.2
			getObjectFromGUID(params.doorTile_GUID).setPosition(position)
			getObjectFromGUID(params.doorTile_GUID).unlock()
			getObjectFromGUID(params.doorTile_GUID).flip()
			wait(3)
			getObjectFromGUID(params.doorTile_GUID).lock()

			return 1
		end
		startLuaCoroutine(self, "flipDoorTile")
	end




	------------------------------------------
	--Room Tiles into parameter if not present
	------------------------------------------
	for _,tAdjacent in ipairs(params.adjacentTiles) do
		local IsSpawned = 0
		for _,obj in ipairs(AllObjects) do
			if tAdjacent == obj.getName() then
				IsSpawned = 1
				break
			end
		end

		if IsSpawned == 0 then
			for _,tMapTiles in ipairs(ScenarioInformation.mapTiles) do
				if tMapTiles.tile == tAdjacent then
					table.insert(params_MapTiles, tMapTiles)
				end
			end
		end
	end




	------------------------------------------
	--Door Tiles into Parameter if not present
	------------------------------------------
	params_DoorTiles = ScenarioInformation.doorTiles
	if ScenarioInformation.doorTiles ~= nil then
		for iDoorTiles=#params_DoorTiles,1,-1 do
			local tType = params_DoorTiles[iDoorTiles]
			for iTiles=#tType.tiles,1,-1 do
				local tTiles = tType.tiles[iTiles]
				local ToDelete = 1

				for _,tAdjacent in ipairs(tTiles.adjacentTiles) do
					--Keep Door Tiles adjacent to potentially spawning tiles
					for _,params_AdjacentTiles in ipairs(params.adjacentTiles) do
						if tAdjacent == params_AdjacentTiles then
							ToDelete = 0
							break
						end
					end
				end

				if ToDelete == 0 then
					for _,tAdjacent in ipairs(tTiles.adjacentTiles) do
						for _,obj in ipairs(AllObjects) do
							if tAdjacent == obj.getName() then
								ToDelete = 1
								break
							end
						end
					end
				end

				if ToDelete == 1 then
					table.remove(tType.tiles, iTiles)
				end
			end
		end
	end




	------------------
	--Spawn Room Tiles
	------------------
	spawnRoomTiles(params_MapTiles,params_DoorTiles)




	----------------
	--Spawn Monsters
	----------------
	--Only Send maptile information for adjacentTiles
	for _,tAdjacent in ipairs(params.adjacentTiles) do
		for _,tMapTiles in ipairs(ScenarioInformation.mapTiles) do
			if tAdjacent == tMapTiles.tile then
				table.insert(params_Monsters,tMapTiles)
				break
			end
		end
	end
	spawnMonsters(params_Monsters,ScenarioInformation.gridType,params.adjacentTiles)



	----------
	--Clean Up
	----------
	function cleanUpParameters()
		wait(Delay_Delete)
		IsBusy = 0
		return 1
	end
	startLuaCoroutine(self, "cleanUpParameters")
end
