	for _,i in ipairs(params) do
		i.type				= "Custom_Token"
		i.position			= position
		i.rotation			= {x=0, y=180, z=0}
		i.scale				= {x=3, y=1, z=3}
		i.sound				= false
		i.snap_to_grid		= true
		i.callback			= "spawnCallback"
		i.callback_owner	= self
		i.params			= {lock = true}

		spawnObject(i).setCustomObject(i)
		position.x = position.x + 10.5
	end
end

