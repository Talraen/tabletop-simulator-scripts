--Spawn Monsters
function spawnMonsters(params_MapTiles,GridType,params_AdjacentTiles)
	function selfFunction()
		wait(Delay_Spawning)

		for _,tMapTiles in ipairs(params_MapTiles) do
			for iUnspawned,tUnspawned in ipairs(Tiles_MonsterUnspawned) do
				if tUnspawned == tMapTiles.tile then
					table.remove(Tiles_MonsterUnspawned, iUnspawned)

					if tMapTiles.monsters ~= nil then
						for _,tMonsters in ipairs(tMapTiles.monsters) do
							if tMonsters.tiles[1].order ~= nil then
								for _,tAdjacent in ipairs(params_AdjacentTiles) do
									if tMonsters.tiles[1].order[tAdjacent] ~= nil then
										table.sort(
											tMonsters.tiles
											,function(a,b)
												return a.order[tAdjacent] < b.order[tAdjacent]
											end
											)
										break
									end
								end
							end

							for _,tIndividual in ipairs(tMonsters.tiles) do
								if tIndividual.numCharacters[NumCharacters] ~= nil then
									local params = {}

									--Set Parameters
									params.monsterName = tMonsters.name

									if TypeMonster == Constant_TypeMonster_Standee then
										params.monsterType = "Standee"
									elseif TypeMonster == Constant_TypeMonster_Hex then
										if GridType == Constant_Grid_Horizontal then
											params.monsterType = "Horizontal"
										elseif GridType == Constant_Grid_Vertical then
											params.monsterType = "Vertical"
										end
									end

									if tIndividual.numCharacters[NumCharacters] == Constant_LevelMonster_Normal then
										params.monsterDifficulty = "normal"
									elseif tIndividual.numCharacters[NumCharacters] == Constant_LevelMonster_Elite then
										params.monsterDifficulty = "elite"
									end

									params.monsterPosition = tIndividual.position

									--Call Spawn Monster
									getObjectFromGUID(Scripts_GUID).call("spawnMonster", params)
								end
							end
						end
					end

					break
				end
			end
		end

		return 1
	end
	startLuaCoroutine(self, "selfFunction")
end




--Spawn Room All Tile Types
function spawnRoomTiles(MapTiles,DoorTiles)
	Timer.destroy("BagCleanUp" .. self.getGUID())




